<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nrv.healthalert" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Settings"></activity>
        <activity android:name=".AlertDialog"></activity>
        <service android:name=".HelathAlertCallback"></service>
        <receiver android:name=".MainActivity$getFromService"></receiver>
        <receiver
            android:name=".AlertDialog$AlarmReceiver"
            android:exported="false">
        </receiver>
    </application>

</manifest>
